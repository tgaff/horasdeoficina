<div class='panel-heading'>
  <%= heading %>
</div>
<div class='panel-body'>
  <table class='table'>
    <thead>
      <tr>
        <th>Course Title</th>
        <th colspan="3"></th>
      </tr>
    </thead>
     <tbody>
      <% specific_courses.each do |course| %>
        <tr>
          <td><%= course.title %></td>
          <td><%= link_to 'Show', course %></td>
          <td><%= link_to 'Edit', edit_course_path(course) %></td>
          <td><%= link_to 'Edit my calendar', course_edit_calendar_path(course) %></td>
          <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>
