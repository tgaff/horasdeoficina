<h1 class="page-header">Editing Course</h1>

<%= render 'form' %>
<div id='scheduling'>
  <h2 class="page-header">Scheduling</h2>
    <h3>Optimal meeting times (calculated)</h3>
    <%=image_tag('calendar.png', alt: 'optimal meeting times', class: 'mini-cal') %>
    <h3>Edit my available times</h3>
    <%= link_to  course_edit_calendar_path(@course) do %>
      <%=image_tag('calendar.png', alt: 'optimal meeting times', class: 'mini-cal') %>
    <% end %>
</div>

<div id='participants'>
  <h2 class="page-header">Course Participants</h2>
  <%= render 'participants_list' %>
</div>

<br>

<%= link_to 'Show', @course %> |
<%= link_to 'Back', courses_path %>


